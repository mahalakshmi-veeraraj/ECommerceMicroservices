package scenario1;

public class EmployeeFacade {
	private EmployeeDAO employeeDAO;

	public EmployeeFacade() {
		this.employeeDAO = new EmployeeDAO();
	}

	public void insert() {
		employeeDAO.insert();
	}

	public Employee getEmployeeDetails(int empId) {
		return employeeDAO.getEmployeeDetails(empId);
	}
}
