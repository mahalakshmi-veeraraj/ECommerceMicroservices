package command;

import invoker.AirConditioner;

public class TurnOffACCommand implements Command {
	private AirConditioner ac;

	public TurnOffACCommand(AirConditioner ac) {
		this.ac = ac;
	}

	@Override
	public void execute() {
		ac.turnoff();
	}

	@Override
	public void undo() {
		ac.turnOff();
	}

}
