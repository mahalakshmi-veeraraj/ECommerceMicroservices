package interpreter;

public class Client {
	public static void main(String[] args) {
		Context context = new Context();
		context.put("a", 10);
		context.put("b", 5);

		MultiplyNonTerminalExpression multiplyNonTerminalExpression = new MultiplyNonTerminalExpression(
				new NumberTerminalExpression("a"), new NumberTerminalExpression("b"));
		int result1 = multiplyNonTerminalExpression.interpret(context);
		System.out.println("Result: " + result1);

		SumNonTerminalExpression sumNonTerminalExpression = new SumNonTerminalExpression(
				new NumberTerminalExpression("a"), new NumberTerminalExpression("b"));
		int result2 = sumNonTerminalExpression.interpret(context);
		System.out.println("Result: "+result2);
		
		context.put("a", result1);
		context.put("b", result2);
		
	}
}
